int FFF(string p,int n)
{
	int i,bj,k=0;
	for(i=0;i<p.size();i++)
	{
		for(int j=0;j<16;j++)
		{
			if(p[i]==a[j])
			{
				bj=j+1;
				break;
			}
		}
		k+=(int)pow((float)n,i)*bj;
	}
	return k;
}
int main(){
	int n,m,x;
	string p;
	char l[1000];
	cin>>n;
	cin>>p;
	cin>>m;
	x=FFF(p,n);
	itoa(x,l,m);
	p=l;
	for(int i=0;i<p.size();i++) if(p[i]>='a'&&p[i]<='z') p[i]-=32;
	cout<<p;
}
